{
  "extends": "../tsconfig.app.json",
  "compilerOptions": {
    "types": [
      "node"
    ],
      "emitDecoratorMetadata": true,
      "esModuleInterop": false,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "../src/test.ts",
    "../src/**/*.spec.ts",
    "../projects/**/*.spec.ts"
  ],
  "include": [
    "../src/**/*",
    "../projects/**/*",
      "../assets/fonts/*"
  ],
  "files": [
    "./typings.d.ts"
  ]
}
